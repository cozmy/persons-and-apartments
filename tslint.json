{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": false,
    "no-var-requires": false,
    "no-string-literal": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": "never"
      }
    ]
  }
}
